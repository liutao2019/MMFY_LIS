  <appSettings>
    <!--入口-->
    <add key="wcfAddr" value="http://localhost:8190/lis/" />
    <!--<add key="wcfAddr" value="http://10.21.16.1:7071/" />-->
    <!--显示时间-->
    <add key ="ShowTime" value="20"/>
    <!--卡1名称-->
    <add key ="NoName1" value="就诊卡号"/>
    <!--卡2名称-->
    <add key ="NoName2" value="就诊卡"/>
    <!--微信图片名称-->
    <add key ="WeChatImage" value=""/>
    <!--读卡模式-->
    <add key ="CardReaderMode" value=""/>
    <!--读卡端口-->
    <add key="DevPort" value=""/>
    <!--提示语1-->
    <add key ="Prompt1" value="2、只可打印出已完成检验的报告单，打印完成后请取出全部纸张。"/>
    <!--提示语2-->
    <add key ="Prompt2" value="3、同一报告只可打一次，若需重新打印，请到门诊三楼检验室找工作人员打印报告！"/>
    <!--提示语3-->
    <add key ="Prompt3" value="4、若遇特殊情况无法打印报告，请到门诊三楼检验室找工作人员打印报告！。"/>
    <!--提示语4-->
    <add key ="Prompt4" value="5、若无法及时取报告或想查看历史报告记录，可扫描右方本院公众号查看。"/>
    <!--纸张数量-->
    <add key="PaperQuantity" value="200"/>
    <!--纸张预警数量-->
    <add key="PaperWarningQuantity" value="40"/>
    <!--启用键盘 Y 启用 N 禁用-->
    <add key="KeyBoardEnable" value="Y"/>
    <!--启用预览 Y 启用 N 禁用-->
    <add key="PreviewEnable" value="N"/>
    <!--不打印的仪器报告单(并且提示)，填写仪器id，多个用逗号隔开-->
    <add key="DotNotPrintItrID" value="'10284','10431'" />  
    <!--不打印报告的提示内容信息，如：需要到门诊5楼XX处取报告！-->
    <add key="DotNotPrintItmAndValueText" value="需要到1楼人工窗口打印！"/>
    <!--是否自动打印-->
    <add key="AutoPrint" value="是"/>
    <!--打报告方式-->
    <add key="PrintType" value="1"/>
    <!--背景图-->
    <add key="BackgroundImage" value=""/>
    <!--标题-->
    <add key="TitleImage" value=""/>
    <!--管理员密码-->
    <add key="SystemPassword" value="123456"/>

    <!--所有配置是否保存在安装目录下-->
    <add key="AppPathSetting_UseAppPath" value="1" />

    <!--<add key="CardReaderMode" value="MT3"/>-->
    <add key="CardType" value="2"/>
    <add key="SectionAddr" value="1"/>
    <add key="BlockAddr" value="0"/>
    <add key="SecKey" value="FACED0614321"/>
    <!--自动适应纸张大小-->
    <add key="AutoFitPaperSize" value="1"/>
  </appSettings>
